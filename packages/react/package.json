{
  "name": "@spotify-confidence/react",
  "license": "Apache-2.0",
  "version": "0.0.13",
  "files": [
    "client.cjs",
    "client.cjs.map",
    "client.mjs",
    "client.mjs.map",
    "client.d.ts",
    "server/server.cjs",
    "server/server.cjs.map",
    "server/server.mjs",
    "server/server.mjs.map",
    "server/server.d.ts",
    "CHANGELOG.md",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "bundle": "rollup -c && ../../validate-api.sh",
    "build": "tsc",
    "prepack": "yarn build && yarn bundle && mv dist/client.* . && mkdir -p ./server && mv dist/server.* ./server/",
    "postpack": "rm -rf ./server client.*",
    "clean": "rm -rf {build,dist}"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "peerDependencies": {
    "@spotify-confidence/sdk": ">=0.1.4 <=0.2.2",
    "react": "^18 || ^19"
  },
  "devDependencies": {
    "@spotify-confidence/sdk": "workspace:*",
    "@types/react": "^18",
    "react": "^19",
    "rollup": "4.24.0",
    "typescript": "5.1.6"
  },
  "exports": {
    "./client": {
      "import": "./build/client.js",
      "types": "./build/client.d.ts"
    },
    "./server": {
      "import": "./build/server.js",
      "types": "./build/server.d.ts"
    }
  },
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "exports": {
      ".": {
        "import": "./client.mjs",
        "require": "./client.cjs",
        "types": "./client.d.ts"
      },
      "./server": {
        "import": "./server/server.mjs",
        "require": "./server/server.cjs",
        "types": "./server/server.d.ts"
      }
    }
  },
  "dependencies": {
    "server-only": "^0.0.1"
  },
  "main": "client.cjs",
  "module": "client.mjs",
  "types": "client.d.ts"
}
