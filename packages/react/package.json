{
  "name": "@spotify-confidence/react",
  "license": "Apache-2.0",
  "version": "0.0.13",
  "files": [
    "dist/*",
    "server/*"
  ],
  "scripts": {
    "bundle": "rollup -c && ../../validate-api.sh",
    "build": "tsc",
    "prepack": "yarn build && yarn bundle && ./scripts/prepack-server.sh",
    "postpack": "rm -rf ./server",
    "clean": "rm -rf {build,dist}"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "peerDependencies": {
    "@spotify-confidence/sdk": ">=0.1.4 <=0.2.2",
    "react": "^18 || ^19"
  },
  "devDependencies": {
    "@spotify-confidence/sdk": "workspace:*",
    "@types/react": "^18",
    "react": "^19",
    "rollup": "4.24.0",
    "typescript": "5.1.6"
  },
  "exports": {
    ".": {
      "import": "./build/client.js",
      "types": "./build/client.d.ts"
    },
    "./server": {
      "import": "./build/server.js",
      "types": "./build/server.d.ts"
    }
  },
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/client.mjs",
        "require": "./dist/client.cjs",
        "types": "./dist/client.d.ts"
      },
      "./server": {
        "import": "./server/index.mjs",
        "require": "./server/index.cjs",
        "types": "./server/index.d.ts"
      }
    }
  },
  "dependencies": {
    "server-only": "^0.0.1"
  },
  "main": "dist/client.cjs",
  "module": "dist/client.mjs",
  "types": "dist/client.d.ts"
}
